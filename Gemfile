# A sample Gemfile
source "https://rubygems.org"

# Load configuration from .env files
gem 'dotenv', :groups => [:development, :test]
# Tests
gem 'rspec', :groups=>[:test]

# sinatra as basic web server
gem 'sinatra'

# Additional sinatra helpers
gem 'sinatra-contrib'

## Service integrations follow

# Talk to github
gem 'octokit'
gem 'freshdesk'
gem 'xml-simple' #parse xml
